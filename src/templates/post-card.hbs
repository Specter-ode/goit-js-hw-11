{{#each this}}
<div class="photo-card">
  <img class='image' src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="290" height="200"/>
  <div class="info">
    <p class="info-item">
      <b>Likes</b>{{likes}}
    </p>
    <p class="info-item">
      <b>Views</b>{{views}}
    </p>
    <p class="info-item">
      <b>Comment</b>{{comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b>{{downloads}}
    </p>
  </div>
</div>
{{/each}}